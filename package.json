{
  "name": "mids",
  "version": "0.0.1",
  "description": "Multiple Interpretation Data Store",
  "main": "build/index.js",
  "types": "build/index.d.js",
  "scripts": {
    "start": "node -r ts-node/register src/index.ts",
    "build": "rm -rf build && mkdir build && npm run build:ts && npm run build:ts.d",
    "build:ts": "tsc --project tsconfig.build.json --outDir build",
    "build:ts.d": "tsc -d --project tsconfig.build.json --outDir build --emitDeclarationOnly",
    "test": "mocha",
    "cov": "nyc mocha",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/averman/mids.git"
  },
  "keywords": [
    "datastructure",
    "storage"
  ],
  "author": "averman",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/averman/mids/issues"
  },
  "homepage": "https://github.com/averman/mids#readme",
  "dependencies": {
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.1",
    "@types/rimraf": "^2.0.2",
    "@types/uuid": "^3.4.5",
    "typescript": "^3.5.3"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "excludeNodeModules": true,
    "instrument": true,
    "sourceMap": true
  }
}
